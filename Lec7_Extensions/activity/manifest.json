{
    "name": "Social Media Blocker",
    "version": "1.0",
    "description": "An Extension to block Social Media Handles!!",
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "web_accessible_resources": [
        "images/*"
    ]
}